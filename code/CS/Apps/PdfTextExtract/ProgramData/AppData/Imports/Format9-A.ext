        <Tsd Name="ResidueSettlement" IsElement="True">
          <Cmd Code="SetTextStart($priorEnd)" />
          <Cmd Code="SetTextEnd(fees &amp; adjustments analysis plant contacts, exclude)" />
          <Cmd Code="LocateToken(settlement residue mmbtu,end,required)" />
          <Cmd Code="TokenizeNextLine(advanceToEol,numericOnly,sr[signedParen1])" />
