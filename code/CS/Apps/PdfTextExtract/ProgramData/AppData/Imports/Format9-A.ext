<ExtractSpecSet>
  <ExtractSpec Name="Format9-A" RecogSpecName="Format9" Desc="Enable Wet/Damp Gas Settlement Statement">
    <Tsd Name="ReportUnit" Iterate="True" IsElement="True" ElementName="ReportUnit">
      <Cmd Code="SetTextStart(settle_gas_stmt.rpt)" />
      <Cmd Code="SetTextEnd(totals [dec] [dec] [eol])" />
      <TsdSet>
        <Tsd Name="Header" IsElement="True" ElementName="ReportHeader">
          <Cmd Code="SetTextStart()" />
          <Cmd Code="SetTextEnd(lease &amp; contract information, exclude)" />
          <Cmd Code="LocateToken(printed:,required,end)" />
          <Cmd Code="ExtractNextToken([date]DatePrinted.MM/DD/YYYY,required)" />
          <Cmd Code="ExtractNextToken([time]TimePrinted.H24:MM:SS,required)" />
        </Tsd>
        <Tsd Name="LeaseAndContract" IsElement="True" ElementName="LeaseAndContract">
          <Cmd Code="SetTextStart($priorEnd)" />
          <Cmd Code="SetTextEnd(settlement summary, exclude)" />
          <Cmd Code="LocateToken(pressure base [eol],end,required)" />
          <Cmd Code="ExtractNextToken(ContractBtuCondition,required)" />
          <Cmd Code="TokenizeNextLine(advanceToEol,8)" />
          <Cmd Code="ExtractStoredToken([mm/yyyy]AccountingMonth.MM/DD/YYYY,0,required,remove)" />
          <Cmd Code="ExtractStoredToken([mm/yyyy]ProductionMonth.MM/DD/YYYY,0,required,remove)" />
          <Cmd Code="ExtractStoredToken([dec]ContractPressureBase,last,required,remove)" />
          <Cmd Code="ExtractStoredToken(ContractNumber,last,required,remove)" />
          <Cmd Code="ExtractStoredToken(State,last,required,remove)" />
          <Cmd Code="ExtractStoredToken([dec]AllocationDecimal,last,required,remove)" />
          <Cmd Code="ExtractStoredToken(LeaseNumber,last,required,remove)" />
          <Cmd Code="ExtractStoredToken(LeaseName,join,required,remove)" />
        </Tsd>
        <Tsd Name="SettlementSummary" IsElement="True" ElementName="SettlementSummary">
          <Cmd Code="SetTextStart($priorEnd)" />
          <Cmd Code="SetTextEnd(wellhead information liquid settlement,exclude)" />
          <Cmd Code="LocateToken(operator name:,end,required)" />
          <Cmd Code="ExtractNextLine(OperatorName,advanceToEol,required)" />
          <Cmd Code="LocateToken(operator id:,end,required)" />
          <Cmd Code="ExtractNextToken(OperatorId,required)" />
          <Cmd Code="LocateToken(value value adjustments tax value [eol],end,required)" />
          <Cmd Code="ExtractNextLine(CounterPartyName,advanceToEol,required)" />
          <Cmd Code="LocateToken(ctr pty name: [eol],end,required)" />
