<AppConfig>
  <ProgramConfigSet>
    <ProgramConfig ConfigName="WinServiceHost">
      <CISet>
        <CIGroup Name="Options">
          <CI K="WindowsServiceName" V="WinServiceHost" />
          <CI K="TasksDbSpecPrefix" V="Tasks" />
          <CI K="NotifyDbSpecPrefix" V="Notify" />
          <CI K="NotifyConfigSetName" V="IM-AppIntegrationOps" />
          <CI K="DefaultNotificationEvent" V="WebServiceDefault" />
          <CI K="TaskScheduleMode" V="FromDatabase" />
          <CI K="NotifyConfigMode" V="FromDatabase" />
          <CI K="LogConfigDbSpec" V="Log" />
          <CI K="WSHTaskProfile" V="FastTest" />
          <CI K="TaskProfile" V="Normal" />
          <CI K="SleepInterval" V="1000" />
          <CI K="UseAlerter" V="True" />
          <CI K="AlerterPath" V="C:\Gulfport Windows Services\ServiceAlert\Org.ServiceAlert.exe" />
          <CI K="TaskLoadIntervalSeconds" V="3600" />
          <CI K="RunWebService" V="True" />
          <CI K="RunUnitDiagnosticsMode" V="Debug" />
          <CI K="~Deserialize_DebugBreak" V="NotifyConfigSet" />
          <CI K="~Serialize_DebugBreak" V="UIState" />
          <CI K="$DEVPATH$" V="C:\DEV-MAIN\Main" />
          <CI K="RunActionSetAtStartup" V="True" />
          <CI K="SuppressNonErrorOutput" V="False" />
          <CI K="SuppressTaskReload" V="True" />
        </CIGroup>
        <CIGroup Name="Logging">
          <CI K="LogMethod" V="Database" />
          <CI K="LogPath" V="" />
          <CI K="LogFileFrequency" V="Daily" />
          <CI K="LogFileSizeManagementMethod" V="Aging" />
          <CI K="LogFileSizeManagementAgent" V="Logger" />
          <CI K="LogFileAgeMaxDays" V="20" />
          <CI K="LogFileSizeMax" V="0" />
          <CI K="LogFileSizeTrim" V="0" />
        </CIGroup>
        <CIGroup Name="LogConfigDbSpec">
          <CI K="LogDbServer" V="OKC1EDW2001" />
          <CI K="LogDbDsn" V="" />
          <CI K="LogDbName" V="Logging" />
          <CI K="LogDbUserId" V="" />
          <CI K="LogDbPassword" V="" />
          <CI K="LogDbType" V="SqlServer" />
          <CI K="LogDbUseWindowsAuth" V="True" />
        </CIGroup>
        <CIGroup Name="WsHostConfigWsSpec">
          <CI K="WsHostWsBinding" V="BasicHttp" />
          <CI K="WsHostWsHost" V="localhost" />
          <CI K="WsHostWsPort" V="32101" />
          <CI K="WsHostWsServiceName" V="" />
        </CIGroup>
        <CIGroup Name="TasksConfigDbSpec">
          <CI K="TasksDbServer" V="OKC1EDW2001" />
          <CI K="TasksDbName" V="TaskScheduling" />
          <CI K="TasksDbUseWindowsAuth" V="True" />
        </CIGroup>
        <CIGroup Name="NotifyConfigDbSpec">
          <CI K="NotifyDbServer" V="OKC1EDW2001" />
          <CI K="NotifyDbName" V="Notifications" />
          <CI K="NotifyDbUseWindowsAuth" V="True" />
        </CIGroup>
        <CIGroup Name="DefaultConfigSmtpSpec">
          <CI K="DefaultSmtpServer" V="smtp.gulfportenergy.com" />
          <CI K="DefaultSmtpPort" V="25" />
          <CI K="DefaultSmtpUserID" V="" />
          <CI K="DefaultSmtpPassword" V="" />
          <CI K="DefaultEnableSSL" V="False" />
          <CI K="DefaultPickUpFromIIS" V="False" />
          <CI K="DefaultAllowAnonymous" V="True" />
          <CI K="DefaultEmailFromAddress" V="sbolt@gulfportenergy.com" />
        </CIGroup>
      </CISet>
      <ConfigListSet>
        <ConfigList Name="TasksToRun">
          <LI V="Dummy" />
        </ConfigList>
      </ConfigListSet>
      <FSActionSet>
        <FSActionGroup Name="Copy_EMK3Tasks" Src="$DEVPATH$\Libs\Gulfport.EMK3.Tasks\bin\Debug" Dst="$MEFCATALOG$\EMK3Tasks">
          <FSAction Name="Step1" FileSystemCommand="Copy" Dst="~\1.0.0.0" Options="Clear,Overwrite">
            <FSItem Name="*.dll" />
            <FSItem Name="*.pdb" />
          </FSAction>
        </FSActionGroup>
        <FSActionGroup Name="Copy_StmtExtractAndLoad" Src="$DEVPATH$\Libs\Gulfport.MWStmt.Tasks\bin\Debug" Dst="$MEFCATALOG$\StmtExtractAndLoad">
          <FSAction Name="Step1" FileSystemCommand="Copy" Dst="~\1.0.0.0" Options="Clear,Overwrite">
            <FSItem Name="*.dll" />
            <FSItem Name="*.pdb" />
          </FSAction>
        </FSActionGroup>
        <FSActionGroup Name="Copy_EMK3ImportData" Src="$DEVPATH$\Libs\Gulfport.MWStmt.Tasks\bin\Debug" Dst="$MEFCATALOG$\EMK3ImportData">
          <FSAction Name="Step1" FileSystemCommand="Copy" Dst="~\1.0.0.0" Options="Clear,Overwrite">
            <FSItem Name="*.dll" />
            <FSItem Name="*.pdb" />
          </FSAction>
        </FSActionGroup>
        <FSActionGroup Name="Copy_ColoradoData" Src="$DEVPATH$\Libs\Gulfport.Colorado.Tasks\bin\Debug" Dst="$MEFCATALOG$\CoProdReportExtract">
          <FSAction Name="Step1" FileSystemCommand="Copy" Dst="~\1.0.0.0" Options="Clear,Overwrite">
            <FSItem Name="*.dll" />
            <FSItem Name="*.pdb" />
          </FSAction>
        </FSActionGroup>
        <FSActionGroup Name="Copy_NonOpData" Src="$DEVPATH$\Libs\Gulfport.NonOp.Tasks\bin\Debug" Dst="$MEFCATALOG$\NonOp">
          <FSAction Name="Step1" FileSystemCommand="Copy" Dst="~\1.0.0.0" Options="Clear,Overwrite">
            <FSItem Name="*.dll" />
            <FSItem Name="*.pdb" />
          </FSAction>
        </FSActionGroup>
      </FSActionSet>
      <TaskConfigurations>
        <TaskConfigSet Mode="FastTest">
          <Task Active="True" Name="EMK3DataImport">
            <TaskSchedule>
              <TaskScheduleElement IsActive="True" ExecutionType="RunImmediate" FrequencySeconds="0.000" >
                <TaskDateTime IntervalType="NotSet" />
                <TaskCalendar WeekdayControl="FTTTTTF" OrdinalControl="TFFFFFFFFFF" SpecificDays="" PeriodContext="NotSet" HolidayAction="RunOnHoliday" />
                <TaskRunControl MaxRunTimeSeconds="0" MaxExecutions="0" AllowConcurrent="False" />
              </TaskScheduleElement>                 
            </TaskSchedule>
            <TaskParmSet>
              <TaskParm K="EMK3DbSpecPrefix" V="EMK3Test" />
              <TaskParm K="MWStmtDbSpecPrefix" V="MWStmt" />
              <TaskParm K="ProductionDate" V="11/01/2015" />
            </TaskParmSet>
          </Task>
          <Task Active="False" Name="Dummy">
            <TaskSchedule>
              <TaskScheduleElement IsActive="False" ExecutionType="RunOnFrequency" FrequencySeconds="30.000" >
                <TaskDateTime StartDate="02/01/2016" StartTime="08:00" EndDate="12/31/2016" EndTime="17:00" IntervalType="DailyInterval" />
                <TaskCalendar WeekdayControl="FTTTTTF" OrdinalControl="TFFFFFFFFFF" SpecificDays="" PeriodContext="NotSet" HolidayAction="RunOnHoliday" />
                <TaskRunControl MaxRunTimeSeconds="0" MaxExecutions="0" AllowConcurrent="True" />
              </TaskScheduleElement>                 
            </TaskSchedule>
            <TaskParmSet>
              <TaskParm K="Parm1" V="ParmValue1" />
              <TaskParm K="Parm2" V="ParmValue2" />
              <TaskParm K="Parm3" V="ParmValue3" />
            </TaskParmSet>
          </Task>
          <TaskHolidayCalendar Values="20160101,20160325,20160530,20160704,20160905,20161124,20161125,20161223,20161226,20161230" />
        </TaskConfigSet>
      </TaskConfigurations>
      <NotifyConfigSet Name="Default">
        <NotifyConfig Name="Default" SupportEmail="kroper@gulfportenergy.com" SupportPhone="405-242-4411">
          <NotifyGroupSet>
            <NotifyGroup Name="Ops1">
              <NotifyPerson Name="Kelly Roper" IsActive="True" EmailAddress="kroper@gulfportenergy.com" IsEmailActive="True" SmsNumber="" IsSmsActive="False" />
              <NotifyPerson Name="Steve Bolt" IsActive="True" EmailAddress="sbolt@gulfportenergy.com" IsEmailActive="True" SmsNumber="" IsSmsActive="False" />
            </NotifyGroup>
          </NotifyGroupSet>
          <NotifyEventSet>
            <NotifyEvent Name="Default" IsActive="True" DefaultSubject="WinService Catch All Error">
              <NotifyGroupReference Name="Ops1" IsActive="True" />
            </NotifyEvent>
          </NotifyEventSet>
        </NotifyConfig>
      </NotifyConfigSet>
    </ProgramConfig>
  </ProgramConfigSet>
</AppConfig>